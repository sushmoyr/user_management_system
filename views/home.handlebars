<table class="table table-bordered">
    <thead class="thead-dark">
    <tr>
        <th scope="col">#</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Email</th>
        <th scope="col">Phone</th>
        <th scope="col" class="text-center">Action</th>
    </tr>
    </thead>
    <tbody>

    {{#each users}}
    <tr>
        <th scope="row">{{id}}</th>
        <td>{{first_name}}</td>
        <td>{{last_name}}</td>
        <td>{{email}}</td>
        <td>{{phone}}</td>
        <td class="text-center">
            <a href="#" class="btn btn-light btn-sm"><i class="bi bi-eye"></i>View</a>
            <a href="#" class="btn btn-light btn-sm"><i class="bi bi-pencil"></i>Edit</a>
            <a href="#" class="btn btn-light btn-sm"><i class="bi bi-person"></i>Delete</a>
        </td>
    {{/each}}
    </tbody>
</table>